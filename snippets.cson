".source.js":
  "React: import empty()":
    prefix: "_i"
    body: "import ${1} from '${2}';"
  "React: component":
    prefix: "_rc"
    body: """export default class ${1} extends Component {
        render() {
          return <div>
          </div>
        }
      }"""
  "React: componentWillMount()":
    prefix: "_rcwm"
    body: """componentWillMount() {
      ${1}
    }"""
  "console.log()":
    prefix: "log"
    body: "console.log(${1})"  
